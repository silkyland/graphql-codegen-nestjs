{
  "name": "@madsci/graphql-codegen-nestjs",
  "version": "0.0.3",
  "description": "GraphQL Code Generator plugin for @nestjs/graphql",
  "author": "Madscience Ltd",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/madscience/graphql-codegen-nestjs.git"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.build.json",
    "postbuild": "cp package.json README.md dist/",
    "lint": "eslint --ext .ts --ignore-path .gitignore .",
    "lint:fix": "eslint --ext .ts --ignore-path .gitignore . --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "rimraf test/.results && mkdirp test/.results && jest --ci --runInBand",
    "postversion": "npm run postbuild",
    "release": "np"
  },
  "dependencies": {
    "@graphql-codegen/plugin-helpers": "^2.0.0",
    "@graphql-codegen/typescript": "^2.0.0",
    "@graphql-codegen/visitor-plugin-common": "^2.0.0",
    "auto-bind": "^4.0.0"
  },
  "peerDependencies": {
    "graphql": "^15.1.0"
  },
  "devDependencies": {
    "@graphql-codegen/testing": "1.17.7",
    "@types/jest": "27.0.2",
    "@types/node": "14.17.21",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-promise": "5.1.0",
    "eslint-plugin-standard": "4.1.0",
    "graphql": "15.6.1",
    "graphql-tools": "8.2.0",
    "husky": "7.0.2",
    "jest": "27.2.5",
    "lint-staged": "11.2.3",
    "mkdirp": "1.0.4",
    "np": "7.5.0",
    "prettier": "2.2.1",
    "ts-jest": "27.0.5",
    "typeorm": "0.2.38",
    "typescript": "4.4.4"
  },
  "main": "index.js",
  "types": "index.d.ts",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js}": "eslint --cache --fix"
  }
}
